#include <stdio.h>
#include <stdlib.h>
#include <conio.h>


int main (){
	FILE *arq;
	char x;
	char y;

	arq = fopen("coordernadas.txt", "r");

	if (arq == NULL){
		printf("Error, não foi possivel abrir o arquivo.");
	}
	else{
		while ((fscanf(arq, "%c %c\n", &x, &y)) != EOF){
			printf("x: %c  e  y: %c", x, y);
		}
	}

	fclose(arq);

	return 0;
}
